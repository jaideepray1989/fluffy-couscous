{
  "version": "0.1",
  "seed_policy": "deterministic",
  "canonical_representation": "point(x,y,t,pen)",
  "tests": [
    {"name": "prefix_points", "params": {"k": [10, 20, 30, 50, 70, 100]}},
    {"name": "prefix_strokes", "params": {"k": [10, 20, 30, 50, 70, 100]}},
    {"name": "stroke_dropout", "params": {"p": [0.1, 0.3, 0.5, 0.7]}},
    {"name": "point_dropout", "params": {"p": [0.1, 0.3, 0.5, 0.7]}},
    {"name": "resample_uniform", "params": {"N": [8, 16, 32, 64]}},
    {"name": "resample_arclength", "params": {"N": [8, 16, 32, 64]}},
    {"name": "subsample_ratio", "params": {"r": [0.25, 0.5, 0.75]}},
    {"name": "simplify_rdp", "params": {"epsilon": [0.5, 1.5, 3.0]}},
    {"name": "simplify_ratio", "params": {"ratio": [0.25, 0.5, 0.75]}},
    {"name": "split_stroke", "params": {"k": [2, 3, 4]}},
    {"name": "merge_strokes", "params": {"threshold": [0.02, 0.05]}},
    {"name": "penup_noise", "params": {"drop_prob": [0.1, 0.2], "insert_prob": [0.1]}},
    {"name": "reverse_strokes", "params": {}},
    {"name": "local_shuffle_strokes", "params": {"window": [2, 3, 5]}},
    {"name": "reverse_points_within_stroke", "params": {}},
    {"name": "time_warp", "params": {"factor": [0.25, 0.5, 1.0, 1.5, 2.0]}},
    {"name": "jitter_xy", "params": {"sigma": [0.01, 0.02, 0.05]}},
    {"name": "quantize_xy", "params": {"bits": [3, 4, 6]}},
    {"name": "timestamp_jitter", "params": {"sigma": [1.0, 2.0]}},
    {"name": "translate", "params": {"dx": [0.1], "dy": [0.1]}},
    {"name": "scale", "params": {"factor": [0.8, 0.9, 1.1, 1.2]}},
    {"name": "rotate", "params": {"degrees": [-30, -15, 15, 30]}},
    {"name": "start_point_shift", "params": {"shift_pct": [10, 25, 50, 75]}}
  ],
  "confusion_sets": [
    {
      "name": "containers",
      "classes": ["cup", "mug", "bowl", "bottle"],
      "pairs": [["cup", "mug"], ["bowl", "cup"]]
    }
  ],
  "scoring": {
    "headline": ["clean_acc", "prefix_auc", "robustness_auc", "worst_cluster_acc"],
    "weights": {
      "prefix_points_robustness": 1.0,
      "prefix_strokes_robustness": 1.0,
      "stroke_dropout_robustness": 1.0,
      "point_dropout_robustness": 1.0,
      "resample_uniform_robustness": 1.0,
      "resample_arclength_robustness": 1.0,
      "subsample_ratio_robustness": 1.0,
      "simplify_rdp_robustness": 0.5,
      "simplify_ratio_robustness": 0.5,
      "split_stroke_robustness": 1.0,
      "merge_strokes_robustness": 1.0,
      "penup_noise_robustness": 1.0,
      "reverse_strokes_robustness": 1.0,
      "local_shuffle_strokes_robustness": 1.0,
      "reverse_points_within_stroke_robustness": 1.0,
      "time_warp_robustness": 1.0,
      "jitter_xy_robustness": 1.0,
      "quantize_xy_robustness": 1.0,
      "timestamp_jitter_robustness": 0.5,
      "translate_robustness": 0.25,
      "scale_robustness": 0.25,
      "rotate_robustness": 0.25,
      "start_point_shift_robustness": 0.5
    }
  }
}
